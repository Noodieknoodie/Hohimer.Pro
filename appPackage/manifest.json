{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.21/MicrosoftTeams.schema.json",
    "manifestVersion": "1.21",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "developer": {
        "name": "Hohimer Wealth Management",
        "websiteUrl": "https://hohimer.com",
        "privacyUrl": "https://hohimer.com/privacy",
        "termsOfUseUrl": "https://hohimer.com/terms"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "HohimerPro${{APP_NAME_SUFFIX}}",
        "full": "401(k) Payment Tracking System"
    },
    "description": {
        "short": "Track advisor fee payments for 401(k) plans",
        "full": "Investment management suite for tracking and managing 401(k) advisor fee payments across multiple providers including John Hancock, Voya, Empower, Principal, and Ascensus."
    },
    "accentColor": "#2563EB",
    "bots": [],
    "composeExtensions": [],
    "staticTabs": [
        {
            "entityId": "index0",
            "name": "Home",
            "contentUrl": "${{TAB_ENDPOINT}}/tab",
            "websiteUrl": "${{TAB_ENDPOINT}}/tab",
            "scopes": [
                "personal",
                "groupChat",
                "team"
            ]
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "${{TAB_DOMAIN}}"
    ]
}